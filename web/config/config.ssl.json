{
  "app": {
    "name": "DADI Web SSL Test"
  },
  "server": {
    "host": "127.0.0.1",
    "port": 5443,
    "protocol": "https",
    "sslPrivateKeyPath": "../ssl/key.pem",
    "sslCertificatePath": "../ssl/cert.pem"
  },
  "api": {
    "enabled": false
  },
  "paths": {
    "events": "workspace/events",
    "helpers": "workspace/helpers",
    "middleware": "workspace/middleware",
    "pages": "workspace/pages",
    "partials": "workspace/partials",
    "public": "workspace/public"
  },
  "cluster": false,
  "debug": false,
  "security": {
    "transportSecurity": true,
    "trustProxy": false
  }
}
